{
    "name": "@certifaction/pdfjs",
    "version": "1.2.0",
    "description": "Convert Mozillas PDF.js to an ES6 module and transform CSS.",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "homepage": "https://certifaction.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/certifaction/js-pdf-viewer.git",
        "directory": "packages/pdfjs"
    },
    "main": "dist/certifaction-pdfjs.common.js",
    "module": "dist/certifaction-pdfjs.esm.js",
    "files": [
        "/dist",
        "/src"
    ],
    "scripts": {
        "clean": "rimraf ./dist",
        "lint": "eslint --fix --ext .js .",
        "test": "jest --verbose",
        "js:compile": "rollup -c build/rollup.config.js --environment BUILD:production",
        "build:js": "npm-run-all js:compile",
        "build": "npm-run-all clean build:js"
    },
    "dependencies": {
        "core-js": "^3.17.3",
        "pdfjs-dist": "^2.9.359"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.56.3",
        "rollup-plugin-copy": "^3.4.0"
    },
    "gitHead": "f5455b748987733baa977142c6b21111fcfa6029"
}
