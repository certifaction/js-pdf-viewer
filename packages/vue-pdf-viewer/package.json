{
    "name": "@certifaction/vue-pdf-viewer",
    "version": "1.3.8",
    "description": "PDF-Viewer for Vue.js based on Mozillas PDF.js",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "homepage": "https://certifaction.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/certifaction/js-pdf-viewer.git",
        "directory": "packages/vue-pdf-viewer"
    },
    "main": "dist/certifaction-vue-pdf-viewer.common.js",
    "module": "dist/certifaction-vue-pdf-viewer.esm.js",
    "files": [
        "/dist",
        "/src"
    ],
    "scripts": {
        "clean": "rimraf ./dist",
        "banner": "node build/banner.js",
        "lint": "eslint --fix --ext .js,.vue .",
        "js:compile": "rollup -c build/rollup.config.js --environment BUILD:production",
        "build:js": "npm-run-all js:compile",
        "build": "npm-run-all clean build:js banner"
    },
    "dependencies": {
        "@certifaction/pdfjs": "^1.3.8",
        "@mdi/js": "^7.0.96",
        "core-js": "^3.26.1"
    },
    "peerDependencies": {
        "vue": "^2.7",
        "vue-i18n": "^8.28"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/preset-env": "^7.20.2",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^23.0.3",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "autoprefixer": "^10.4.13",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.28.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^9.8.0",
        "glob": "^8.0.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.79.1",
        "rollup-plugin-vue": "^5.1.9",
        "vue": "^2.7.14",
        "vue-template-compiler": "^2.7.14"
    },
    "gitHead": "f5455b748987733baa977142c6b21111fcfa6029"
