{
    "name": "@certifaction/vue-pdf-viewer",
    "version": "1.2.0",
    "description": "PDF-Viewer for Vue.js based on Mozillas PDF.js",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "homepage": "https://certifaction.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/certifaction/js-pdf-viewer.git",
        "directory": "packages/vue-pdf-viewer"
    },
    "main": "dist/certifaction-vue-pdf-viewer.common.js",
    "module": "dist/certifaction-vue-pdf-viewer.esm.js",
    "files": [
        "/dist",
        "/src"
    ],
    "scripts": {
        "clean": "rimraf ./dist",
        "banner": "node build/banner.js",
        "lint": "eslint --fix --ext .js,.vue .",
        "js:compile": "rollup -c build/rollup.config.js --environment BUILD:production",
        "build:js": "npm-run-all js:compile",
        "build": "npm-run-all clean build:js banner"
    },
    "dependencies": {
        "@certifaction/pdfjs": "^1.2.0",
        "@mdi/js": "^5.9.55",
        "core-js": "^3.14.0"
    },
    "peerDependencies": {
        "vue": "^2.6",
        "vue-i18n": "^8.24"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "autoprefixer": "^10.2.6",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.28.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-vue": "^7.11.1",
        "glob": "^7.1.7",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.51.2",
        "rollup-plugin-vue": "^5.1.9",
        "vue": "^2.6.14",
        "vue-template-compiler": "^2.6.14"
    },
    "gitHead": "f5455b748987733baa977142c6b21111fcfa6029"
