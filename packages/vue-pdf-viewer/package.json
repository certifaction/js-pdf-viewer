{
    "name": "@certifaction/vue-pdf-viewer",
    "version": "1.7.0",
    "description": "PDF-Viewer for Vue.js based on Mozillas PDF.js",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "homepage": "https://certifaction.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/certifaction/js-pdf-viewer.git",
        "directory": "packages/vue-pdf-viewer"
    },
    "type": "module",
    "main": "dist/certifaction-vue-pdf-viewer.common.js",
    "module": "dist/certifaction-vue-pdf-viewer.esm.js",
    "files": [
        "/dist",
        "/src"
    ],
    "scripts": {
        "clean": "rimraf ./dist",
        "banner": "node build/banner.js",
        "lint": "eslint --fix --ext .js,.vue .",
        "js:compile": "rollup -c build/rollup.config.js --environment BUILD:production",
        "build:js": "npm-run-all js:compile",
        "build": "npm-run-all clean build:js banner"
    },
    "dependencies": {
        "@certifaction/pdfjs": "^1.7.0",
        "@mdi/js": "^7.2.96",
        "core-js": "^3.31.1"
    },
    "peerDependencies": {
        "vue": "^2.7",
        "vue-i18n": "^8.28"
    },
    "devDependencies": {
        "@babel/core": "^7.22.9",
        "@babel/eslint-parser": "^7.22.9",
        "@babel/preset-env": "^7.22.9",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^25.0.3",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.45.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^9.15.1",
        "glob": "^10.3.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.1",
        "rollup": "^3.26.3",
        "rollup-plugin-vue": "^5.1.9",
        "vue": "^2.7.14",
        "vue-template-compiler": "^2.7.14"
    }
}
